 int array_len = sizeof(arr_keys)/sizeof(arr_keys[0]);
 int input_len = sizeof(arr_input)/sizeof(arr_input[0]);

 void computeArraySin_ccm(float * arr_input, float * arr_keys , float * arr_values, int n_lut, int n_input){
     for(int i=0; i<n_input; i++){
         for (int k=0; k<n_lut; k++){
          if (*(arr_keys+k+1) >= *(arr_input+i)){
             output = *(arr_values+k) + (*(arr_values+k+1)-*(arr_values+k))*(*(arr_input+i) - *(arr_keys + k))/(*(arr_keys+k+1)-*(arr_keys+k));
             break;
             };
         };
     }
 }
 void computeArraySin(float * arr_input, float * arr_keys , float * arr_values, int n_lut, int n_input){
     for(int i=0; i<n_input; i++){
         for (int k=0; k<n_lut; k++){
          if (*(arr_keys+k+1) >= *(arr_input+i)){
             output = *(arr_values+k) + (*(arr_values+k+1)-*(arr_values+k))*(*(arr_input+i) - *(arr_keys + k))/(*(arr_keys+k+1)-*(arr_keys+k));
             break;
             };
         };
     }
 }

 void computeArraySin_ram(float * arr_input, float * arr_keys , float * arr_values, int n_lut, int n_input){
     for(int i=0; i<n_input; i++){
         for (int k=0; k<n_lut; k++){
          if (*(arr_keys+k+1) >= *(arr_input+i)){
             output = *(arr_values+k) + (*(arr_values+k+1)-*(arr_values+k))*(*(arr_input+i) - *(arr_keys + k))/(*(arr_keys+k+1)-*(arr_keys+k));
             break;
             };
         };
     }
 }

 void computeArraySin_ram2(float * arr_input, float * arr_keys , float * arr_values, int n_lut, int n_input){
     for(int i=0; i<n_input; i++){
         for (int k=0; k<n_lut; k++){
          if (*(arr_keys+k+1) >= *(arr_input+i)){
             output = *(arr_values+k) + (*(arr_values+k+1)-*(arr_values+k))*(*(arr_input+i) - *(arr_keys + k))/(*(arr_keys+k+1)-*(arr_keys+k));
             break;
             };
         };
     }
 }
 void sine_main()
 {
   computeArraySin(&arr_input[0], &arr_keys[0], &arr_values[0], array_len, input_len);
 }

 void sine_main_ccm()
 {
   computeArraySin_ccm(&arr_input[0], &arr_keys[0], &arr_values[0], array_len, input_len);
 }

 void sine_main_ram()
 {
   computeArraySin_ram(&arr_input[0], &arr_keys[0], &arr_values[0], array_len, input_len);
 }

 void sine_main_ram2()
 {
   computeArraySin_ram2(&arr_input[0], &arr_keys[0], &arr_values[0], array_len, input_len);
 }


